{
	"compilerOptions": {
		"module": "commonjs",
		"lib": ["es6", "dom", "es2017"],
		"target": "es6",
		"noImplicitAny": false,
		"removeComments": false,
		"noUnusedLocals": false,
		"noImplicitThis": true,
		"inlineSourceMap": false,
		"sourceMap": true,
		"outDir": "../out",
		"preserveConstEnums": true,
		"strictNullChecks": false,
		"noUnusedParameters": false,
		"strict": false   /* enable all strict type-checking options */
	}
}
